{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "es2022",
    "lib": ["es2022", "webworker"],
    "types": ["@cloudflare/workers-types", "jest", "node"],
    "strict": true,
    "skipLibCheck": true,
    "outDir": "../../dist/out-tsc"
  },
  "include": ["src/**/*.ts", "jest.config.ts"],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"]
}
